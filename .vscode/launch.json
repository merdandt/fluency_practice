  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "debug",
      "args": [
        "development",
        "--target",
        "lib/main.dart",
        "--dart-define-from-file",
        "env/config/app-config.json"
      ],

    },
    {
      "name": "Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "profile",
      "args": [
        "staging",
        "--target",
        "lib/main.dart",
        "--dart-define-from-file",
        "env/config/app-config.json"
      ],
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "release",
      "args": [
        "production",
        "--target",
        "lib/main.dart",
        "--dart-define-from-file",
        "env/config/app-configd.json"
      ],
     
    }
  ]
}